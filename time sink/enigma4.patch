+eScriptNode * eScriptCompiler::_compare()
+{
+    eScriptNode *r = _factor();
+    if (!r)
+        _error("syntax error");
+
+    while (m_scanner.peek().type == TOKEN_OPERATOR)
+    {
+        const char op = m_scanner.peek().op;
+        const char op2 = m_scanner.peek().op2;
+
+        if (op == '=' || op == '>' || op == '<')
+        {
+            m_scanner.next();
+            r = _createNode(SNT_BINOP, r, _factor(), nullptr);
+            r->varType = r->left->varType;
+
+            if (!r->left || !r->right)
+                _error("syntax error");
+
+            if (r->left->varType == ST_SCALAR && r->right->varType == ST_SCALAR)
+			{
+				if(op == '=')
+				{
+					r->opCode = (op2 == '=' ? OC_COMPARE_EQUAL : (op2 == '>' ? OC_COMPARE_EQUAL_GREATERTHAN : OC_COMPARE_EQUAL_LESSTHAN));
+				}
+				else
+				{
+                    r->opCode = (op == '>' ? OC_COMPARE_GREATERTHAN : OC_COMPARE_LESSTHAN);
+				}
+			}
+//            else if (r->left->varType == ST_VECTOR && r->right->varType == ST_VECTOR)
+//                r->opCode = (op == '*' ? OC_MULV : OC_DIVV);
+            else
+                _error("type mismatch");
+        }
+        else break;
+    }
+
+    return r;
+}
