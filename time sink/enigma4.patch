+
+        for (eU32 i=0; i<segCount; i++)
+            mesh.addLine(oldVtxCount+i, oldVtxCount+(i+1)%segCount, eMaterial::getWireframe());
+
+        mesh.addLine(oldVtxCount, oldVtxCount+segCount/2, eMaterial::getWireframe());
