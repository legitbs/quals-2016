+// AFterDark (mesh) operator
+// ------------------------
+// After Dark type screensaver
+
+#if defined(HAVE_OP_MODEL_AFTERDARK) || defined(eEDITOR)
+eOP_DEF_MODEL(eModelAfterDark, "AfterDark", 'k', 0, 0, eOP_INPUTS())
+    eOP_INIT()
+    {
+		m_lastTime = 0.0f;
+		m_lastCount = 0;
+		m_lastSeed = 0;
+		ranSeed = 0;
+		m_area = 0;
+		m_DidModel3 = true;
+		m_lastModel3Time = 0;
+    }
+
+public:
+	struct tEntry {
+		eU8			mdlIdx;
+        eVector3    position;
+		eF32		CurSpeed;
+		eF32		DesiredZPos;
+    };
+
+	eOP_EXEC2(ENABLE_STATIC_PARAMS,
+        eOP_PAR_INT(eModelAfterDark, count, "Count", 1, 65536, 1,
+		eOP_PAR_FXYZ(eModelAfterDark, area, "Area", eF32_MIN, eF32_MAX, 10, 10, 10,
+		eOP_PAR_FLOAT(eModelAfterDark, mingap, "Min Gap", 0, eF32_MAX, 2,
+        eOP_PAR_LINK_MODEL(eModelAfterDark, modelOp, "Model", eOC_MODEL, eTRUE,
+		eOP_PAR_FLOAT(eModelAfterDark, modelOpPercent, "Model Percent", 0, 1, 0.5,
+        eOP_PAR_LINK_MODEL(eModelAfterDark, modelOp2, "Model2", eOC_MODEL, eFALSE,
+        eOP_PAR_LINK_MODEL(eModelAfterDark, modelOp3, "Model3", eOC_MODEL, eFALSE,
+		eOP_PAR_FLOAT(eModelAfterDark, model3Time, "Model 3 time", 0, eF32_MAX, 2,
+		eOP_PAR_INT(eModelAfterDark, seed, "Seed", 0, 65535, 0,
+		eOP_PAR_FLOAT(eModelAfterDark, time, "Time", eF32_MIN, eF32_MAX, 0,
+		eOP_PAR_END)))))))))))
+    {
+		ePROFILER_ZONE("After Dark Op");
+
+		eU32 TotalCount = count;
+		eF32 popDistSqr = mingap * mingap;
+		m_sceneData.clear();
+
+		m_models.clear();
+		m_models.append(&modelOp->getResult().sceneData);
+		if(modelOp2)
+			m_models.append(&modelOp2->getResult().sceneData);
+		if(modelOp3)
+		{
+			m_models.append(&modelOp3->getResult().sceneData);
+			TotalCount++;
+
+			if(model3Time != m_lastModel3Time)
+			{
+				m_lastModel3Time = model3Time;
+				m_DidModel3 = false;
+			}
+		}
+
+		if(m_entries.size() == 0
+#ifdef eEDITOR
+			|| (time < m_lastTime) || 
+			(TotalCount != m_lastCount) || 
+			(seed != m_lastSeed) ||
+			(area != m_area)
+#endif
+			) {
+			m_lastTime = time;
+			m_lastCount = TotalCount;
+			m_lastSeed = seed;
+			m_area = area;
+
+			ranSeed = seed + 1;
+			eF32 AreaX = area.x;
+			if(mingap > area.x)
+				AreaX = mingap + 1;
+			
+			// reset entries
+			m_entries.clear();
+			m_entries.reserve(count);
+
+			for(eU32 i = 0; i < TotalCount; i++) {
+				tEntry& e = m_entries.append();
+
+				//e.position.x = eRandomF(-AreaX, 0, ranSeed);
+				e.position.x = eRandomF(0, AreaX, ranSeed);
+				e.position.y = eRandomF(0, area.y, ranSeed);
+				e.position.z = eRandomF(0, area.z, ranSeed);
+				e.DesiredZPos = e.position.z;
+
+				//eU32 ModelID = eRandom(0, 100, ranSeed);
+				e.CurSpeed = eRandomF(0.5, 3, ranSeed);
+				e.mdlIdx = (eRandomF(0.0, 1.0, ranSeed) >= modelOpPercent) & 1;
+
+				// test population distance, remove if too close to one
+				bool isnear = false;
+				for(eU32 k = 0; k < (m_entries.size() - 1); k++) 
+					isnear |= (e.position - m_entries[k].position).sqrLength() < popDistSqr;
+				if(isnear)
+					m_entries.resize(m_entries.size() - 1);
+			}
+
+			//guarantee the last one is model 3 if model 3 exists
+			if(modelOp3)
+			{
+				TotalCount = m_entries.size() - 1;
+				m_entries[TotalCount].mdlIdx = 2;
+				//m_entries[TotalCount].position.z = area.z / 2;
+				m_entries[TotalCount].position = area / 2;
+				m_entries[TotalCount].position.x = (mingap*2);
+				m_entries[TotalCount].DesiredZPos = m_entries[TotalCount].position.z;
+				//m_entries[TotalCount].position.y = area.y / 2;
+				//m_entries[TotalCount].position.z = area.z / 2;
+				m_entries[TotalCount].CurSpeed = 5;
+			}
+		}
+
+		//see if we need to start drawing model 3
+		TotalCount = m_entries.size();
+		if(modelOp3)
+		{
+			if((time < model3Time) || m_DidModel3)
+				TotalCount--;
+		}
+
+		// draw
+		eF32 CurVelocity;
+		for(eU32 k = 0; k < TotalCount; k++) {
+			tEntry& entry = m_entries[k];
+
+			eTransform tr;
+			tr.translate(entry.position);
+			CurVelocity = entry.CurSpeed * (m_lastTime - time);
+
+			// test population distance, adjust speed if too close to one
+			eF32 OrigX, OrigZ;
+			OrigX = entry.position.x;
+			OrigZ = entry.position.z;
+
+			entry.position.x -= CurVelocity;
+
+			bool isnear = false;
+			for(eU32 k2 = 0; k2 < m_entries.size(); k2++)
+			{
+				//don't check against ourselves
+				if(k == k2)
+					continue;
+
+				//don't check anything behind us or too far ahead of us
+				if((m_entries[k2].position.x > (entry.position.x + mingap)) || (m_entries[k2].position.x < (entry.position.x - popDistSqr)))
+					continue;
+
+				if(entry.position.z < m_entries[k2].position.z)
+					continue;
+
+				//if our speed is slower then make them move
+				if(entry.CurSpeed < m_entries[k2].CurSpeed)
+					continue;
+
+				if ((entry.position - m_entries[k2].position).sqrLength() < popDistSqr)
+				{
+					entry.position.x = OrigX;
+
+					//move out a bit to try to go around it
+					entry.position.z -= (mingap*2) * (m_lastTime - time);
+
+					isnear = true;
+					break;
+				}
+
+				//validate that if we brought the Z in closer it would cause us to be near
+				if(entry.position.z > entry.DesiredZPos)
+				{
+					entry.position.z = entry.DesiredZPos;
+					if ((entry.position - m_entries[k2].position).sqrLength() < popDistSqr)
+					{
+						entry.position.z = OrigZ;
+						isnear = true;
+						break;
+					}
+					entry.position.z = OrigZ;
+				}
+			}
+
+			//not near and out of alignment of where we want to be
+			if(!isnear && (entry.position.z > entry.DesiredZPos))
+			{
+				entry.position.z += ( (entry.position.z - entry.DesiredZPos) * (m_lastTime - time));
+				//entry.position.z = entry.DesiredPos.z;
+			}
+
+			if(entry.position.x >= area.x)
+			{
+				//if model 3, set it's last time so we stop rendering it and move it back into location
+				if(entry.mdlIdx == 2)
+				{
+					entry.position.z = entry.DesiredZPos;
+					m_DidModel3 = true;
+
+					entry.position.x = 0;
+				}
+				else
+				{
+					entry.position.x = 0;
+					entry.position.y = eRandomF(0, area.y, ranSeed);
+					entry.position.z = eRandomF(0, area.z, ranSeed);
+					entry.DesiredZPos = entry.position.z;
+				}
+
+				do
+				{
+					isnear = false;
+
+					//check we aren't near anything
+					for(eU32 k2 = 0; k2 < m_entries.size(); k2++)
+					{
+						//don't check against ourselves
+						if(k == k2)
+							continue;
+
+						//don't check anything behind us or too far ahead of us
+						if((m_entries[k2].position.x > (entry.position.x + mingap)) || (m_entries[k2].position.x < (entry.position.x - popDistSqr)))
+							continue;
+
+						if ((entry.position - m_entries[k2].position).sqrLength() < popDistSqr)
+						{
+							//move further back
+							entry.position.x -= mingap;
+							isnear = true;
+							break;
+						}
+					}
+				} while(isnear);
+			}
+
+			//merge this entry in
+			m_sceneData.merge(*m_models[entry.mdlIdx], tr);
+		}
+
+		m_lastTime = time;
+    }
+	eOP_EXEC2_END
+
+	eU32			m_lastCount;
+	eU32			m_lastSeed;
+	eU32			ranSeed;
+	eF32			m_lastTime;
+	eF32			m_lastModel3Time;
+	eBool			m_DidModel3;
+	eVector3		m_area;
+    eArray<tEntry>  m_entries;
+	eArray<eSceneData*> m_models;
+eOP_END(eModelAfterDark);
+#endif
 
 
