+        const eBool yClamp = eGetBit(clampBorders, 1);
+        const eU32 texAddr = (xClamp ? eTMF_CLAMPX : eTMF_WRAPX)|(yClamp ? eTMF_CLAMPY : eTMF_WRAPY);
