+    void _freeTextures()
+    {
+        if (m_ownsDiffuse)
+            eGfx->removeTexture2d(m_texDiffuse);
+        if (m_ownsNormal)
+            eGfx->removeTexture2d(m_texNormal);
+        if (m_ownsSpecular)
+            eGfx->removeTexture2d(m_texSpecular);
+        if (m_ownsEnv)
+            eGfx->removeTexture2d(m_texEnv);
